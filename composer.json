{
  "name": "jagdeepbanga/php-google-product-review-feeds",
  "description": "Google Product Review Feed Generator in PHP",
  "keywords": [
    "php",
    "google",
    "product",
    "review",
    "feed",
    "generator"
  ],
  "homepage": "https://github.com/jagdeepbanga/php-google-product-review-feeds",
  "license": "MIT",
  "authors": [
    {
      "name": "jagdeep Singh Banga",
      "email": "banga.jagdeep@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "sabre/xml": "^4.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Jagdeepbanga\\GoogleProductReviewFeed\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jagdeepbanga\\GoogleProductReviewFeed\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": [
      "phpstan analyse -l 6 src tests"
    ],
    "test": [
      "vendor/bin/phpunit --configuration ./phpunit.xml"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
